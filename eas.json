{
    "cli": {
        "version": ">= 16.28.0",
        "appVersionSource": "remote",
        "promptToConfigurePushNotifications": false
    },
    "build": {
        "base": {
            "node": "24.11.0",
            "bun": "1.3.5",
            "cache": {
                "disabled": true
            },
            "channel": "base"
        },
        "dev-client": {
            "extends": "base",
            "distribution": "internal",
            "android": {
                "buildType": "apk"
            },
            "developmentClient": true,
            "environment": "preview",
            "channel": "preview"
        },
        "development": {
            "extends": "base",
            "autoIncrement": true,
            "android": {
                "buildType": "apk"
            },
            "environment": "development",
            "channel": "development"
        },
        "preview": {
            "extends": "base",
            "autoIncrement": true,
            "environment": "preview",
            "channel": "preview"
        },
        "production": {
            "extends": "base",
            "autoIncrement": true,
            "environment": "production",
            "channel": "production"
        }
    },
    "submit": {
        "production": {}
    }
}
